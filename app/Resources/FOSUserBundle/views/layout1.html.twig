<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="keywords" content="multi services gsm, reparation, telephone, OrlÃ©ans" />
        <title>{% block title %} Multi Service Gsm {% endblock title %}</title>
        {% stylesheets '@bootstrap_css' %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('web/css/bootstrap.min.css') }}"/>
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
            <link rel="stylesheet" href="{{ asset('css/footer-distributed-address-and-phones.css') }}"/>
            <link rel="stylesheet" href="{{ asset('css/Admin.css') }}"/>
            <link rel="stylesheet" href="{{ asset('css/publi.css') }}"/>
            <link href="{{ asset ('http://fonts.googleapis.com/css?family=Cookie') }}" rel="stylesheet" type="text/css"/>
            <link rel="stylesheet" href="css/page-acceuil.css"/>    
            <link href="https://fonts.googleapis.com/css?family=Pacifico" rel="stylesheet">
        {% endstylesheets %}
        {% stylesheets '@font_awesome_css' %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>          
        {% endstylesheets %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    {% include '::menu.html.twig' %}
    <body>
        <div class="container">
            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                <a href="{{ path('fos_user_security_logout') }}">
                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                </a>
            
            {% endif %}
        </div>

        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                    <div class="alert alert-{{ type }}">
                        <ul>
                            {% for message in messages %}
                                   <li>
                                        {{ message }}
                                    </li>
                            {% endfor %}
                        </ul>
                    </div>
            {% endfor %}
        {% endif %}

            <h1 style="text-align:center;">Multi Service Gsm </h1>
        <div class=" row rcorners2">
            <div class=" col-sm-7 " >
                {% block fos_user_content %}                    
                {% endblock fos_user_content %}
            </div>
            <div class="col-sm-4 "  >

                {% block fos_user_content1 %}                    
                {% endblock fos_user_content1 %}
            </div>
        </div>

       
        {% javascripts '@jquery' '@bootstrap_js' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts '@MultiServiceGsmFrontBundle/Resources/public/js/*' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% include '::footer.html.twig' %}
    </body>
</html>
